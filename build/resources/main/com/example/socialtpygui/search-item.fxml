<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneSearchItemView" prefHeight="64.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialtpygui.controller.SearchItemController">
   <children>
      <Label fx:id="nameLbl" layoutX="14.0" layoutY="24.0" prefHeight="17.0" prefWidth="207.0" text="Nume" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font></Label>
      <ImageView fx:id="addBtn" fitHeight="30.0" fitWidth="30.0" layoutX="576.0" layoutY="17.0" onMouseClicked="#handlerAddBtn" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@icons/plus-sign.png" />
         </image>
      </ImageView>
      <ImageView fx:id="cancelBtn" fitHeight="30.0" fitWidth="30.0" layoutX="576.0" layoutY="17.0" onMouseClicked="#handlerCancelBtn" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@icons/cross.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
